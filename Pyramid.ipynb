   ],
   "source": [
    "def main():\n",
    "    height = int(input(\"Height: \"))\n",
    "    pyramid(height)\n",
    "\n",
    "def pyramid(n):\n",
    "    for i in range(n):\n",
    "        print(\"#\" * (i + 1))\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
  }
